{
    "name": "faceauth/laravel-face-auth",
    "description": "Laravel package for facial authentication using FaceAPI.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "FaceAuth Team",
            "email": "opensource@faceauth.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "illuminate/support": "^8.0 || ^9.0 || ^10.0"
    },
    "autoload": {
        "psr-4": {
            "FaceAuth\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "FaceAuth\\FaceAuthServiceProvider"
            ]
        }
    }
}
